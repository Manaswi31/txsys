						<sub-object/>
					</object-list>
				</attribute>
			</sub-object>
		</assignment>
	</attribute-assignments>
	<state-variables>
<![CDATA[
Boolean	\sv_traf_on_flag;

]]>
	</state-variables>
	<header-block>
<![CDATA[
#define NO_TRAFFIC (sv_traf_on_flag==OPC_FALSE)
]]>
	</header-block>
	<function-block>
<![CDATA[
static void gen(void)
	{
	Packet * pkptr;
	FIN(gen());
	pkptr = op_pk_create(114);
	op_pk_send(pkptr);
	
	
	op_intrpt_schedule_self(op_sim_time()+VOICE_INTVL, 0);
	
	FOUT;
	}
]]>
	</function-block>
